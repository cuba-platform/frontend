<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="msg://editorCaption"
        focusComponent="form"
        messagesPack="com.company.scr.web.screens.deeplynestedtestentity">
  <data>
    <instance id="deeplyNestedTestEntityDc"
              class="com.company.scr.entity.test.DeeplyNestedTestEntity"
              view="deeplyNestedTestEntity-view">
      <loader/>
    </instance>
  </data>
  <dialogMode height="600"
              width="800"/>
  <layout expand="editActions" spacing="true">
    <form id="form" dataContainer="deeplyNestedTestEntityDc">
      <column width="250px">
        <textField id="nameField" property="name"/>
        <pickerField id="associationO2OattrField" property="associationO2Oattr">
          <actions>
            <action id="lookup" type="picker_lookup"/>
            <action id="clear" type="picker_clear"/>
          </actions>
        </pickerField>
      </column>
    </form>
    <hbox id="editActions" spacing="true">
      <button action="windowCommitAndClose"/>
      <button action="windowClose"/>
    </hbox>
  </layout>
</window>
