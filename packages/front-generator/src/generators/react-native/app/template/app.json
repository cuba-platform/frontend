{
  "expo": {
    "name": "<%=project.namespace%>",
    "slug": "<%=project.namespace%>",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "change.this.to.your.own.package.name",
      "supportsTablet": true
    },
    "android": {
      "package": "change.this.to.your.own.package.name"
    }
  }
}
